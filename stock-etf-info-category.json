  "keyword_triggers": {
    "info_requests": {
      "primary_keywords": ["what is", "tell me about", "info", "information", "details", "about"],
      "symbols": ["AAPL", "GOOGL", "TSLA", "SPY", "QQQ", "ARKK", "VTI"],
      "question_patterns": ["what is [SYMBOL]", "tell me about [SYMBOL]", "[SYMBOL] info", "about [SYMBOL]"]
    },
    "snapshot_requests": {
      "primary_keywords": ["snapshot", "overview", "summary", "quick", "brief", "stats", "metrics"],
      "question_patterns": ["[SYMBOL] snapshot", "quick [SYMBOL] overview", "[SYMBOL] summary"]
    },
    "price_performance_requests": {
      "primary_keywords": ["price", "performance", "returns", "how is", "up", "down", "chart", "doing"],
      "question_patterns": ["[SYMBOL] price", "how is [SYMBOL]", "[SYMBOL] performance", "[SYMBOL] returns", "how much is [SYMBOL] up"]
    },
    "holdings_specific_requests": {
      "primary_keywords": ["holdings", "top holdings", "what does it hold", "companies", "stocks in"],
      "question_patterns": ["[SYMBOL] holdings", "top 10 holdings of [SYMBOL]", "what does [SYMBOL] hold", "companies in [SYMBOL]"],
      "output_filter": "holdings_only"
    },
    "sectors_specific_requests": {
      "primary_keywords": ["sectors", "sector breakdown", "allocation", "what sectors"],
      "question_patterns": ["[SYMBOL] sectors", "sector breakdown of [SYMBOL]", "what sectors does [SYMBOL] hold"],
      "output_filter": "sectors_only"
    },
    "basic_info_requests": {
      "primary_keywords": ["what is", "tell me about", "expense ratio", "fund info"],
      "question_patterns": ["what is [SYMBOL]", "tell me about [SYMBOL]", "[SYMBOL] expense ratio"],
      "output_filter": "info_only"
    }
  },
  "user_intent_mapping": {
    "basic_learning": "User wants to understand what this stock/ETF is",
    "quick_check": "User wants current price and basic metrics",
    "performance_tracking": "User wants to know how the investment is performing",
    "research_starting_point": "User beginning research on this investment",
    "holdings_focused": "User specifically wants to know what the ETF holds",
    "sector_focused": "User wants sector/allocation breakdown"
  },
  "output_selection_logic": {
    "description": "How to determine which output sections to return based on user input",
    "rules": [
      {
        "if_keywords": ["holdings", "top holdings", "companies", "stocks in"],
        "then_output_filter": "holdings_only",
        "return_sections": ["top_holdings"]
      },
      {
        "if_keywords": ["sectors", "sector breakdown", "allocation"],
        "then_output_filter": "sectors_only", 
        "return_sections": ["sector_breakdown"]
      },
      {
        "if_keywords": ["expense ratio", "fees", "cost"],
        "then_output_filter": "info_only",
        "return_sections": ["basic_info"]
      },
      {
        "if_keywords": ["performance", "returns", "how is it doing"],
        "then_output_filter": "performance_only",
        "return_sections": ["historical_performance", "current_metrics"]
      },
      {
        "if_keywords": ["what is", "tell me about", "overview"],
        "then_output_filter": "all",
        "return_sections": ["basic_info", "current_metrics", "historical_performance", "top_holdings", "sector_breakdown"]
      }
    ]
  },
